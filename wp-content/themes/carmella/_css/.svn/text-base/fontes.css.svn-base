/* Fale Conosco */

#faleconosco {
	font-family: 'Open Sans', Helvetica, Arial, sans-serif;
	font-size: 14px;
}

#faleconosco h1 {
	font-family: 'Open Sans', Helvetica, Arial, sans-serif;
	font-size: 36px;
	color: #003333;
	height: 40px;
}

#faleconosco input[type="text"] {
	font-family: 'Open Sans', Helvetica, Arial, sans-serif;
	font-size: 14px;
	padding: 5px;
	margin: 0;
	border: 1px solid rgba(0, 0, 0, .1);
	background: #E8FFEA;
	min-width: 300px;
	color: #666;
}

#faleconosco input[type="text"]:FOCUS {
	background: white;
}

#faleconosco textarea {
	background: #E8FFEA;
	border: 1px solid rgba(0, 0, 0, .1);
}

#faleconosco textarea:FOCUS {
	background: white;
}

button {
	background: #003333;
