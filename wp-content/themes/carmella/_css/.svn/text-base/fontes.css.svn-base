#topo > section > header > h1{
	font-family: 'lobster_1.4regular';
}

section > header > p, .pedido {
	font-family: 'open_sansregular', 'Open Sans', Helvetica, Arial,
		sans-serif;
}

#menu li {
	font-family: 'Open Sans', Helvetica, Arial,
		sans-serif;
    font-size: 22px;	
}

footer {
	font-family: 'Open Sans', Helvetica, Arial,
		sans-serif;
    font-size: 18px;
    text-align: center;
}

footer a {
	color: white;
	text-decoration: none;
}

footer a:hover {
	color: white;
	text-decoration: none;
}

.promocoes h1 {
	font-family: 'Open Sans', Helvetica, Arial,
		sans-serif;
    font-size: 36px;
	font-weight: bold;
	text-align: center;
}

.propaganda1 h1, .propaganda2 h1, .propaganda3 h1 {
	font-family: 'Open Sans', Helvetica, Arial,
		sans-serif;
	font-size: 48px;
	color: #FFCC00;
	text-transform: uppercase;
	font-weight: bold;
}

.propaganda1 h2, .propaganda2 h2, .propaganda3 h2 {
	font-family: 'Open Sans', Helvetica, Arial,
		sans-serif;
	color: #003333;
	font-weight: normal;
	text-align: center;
	font-size: 27px;
	margin: 0;
}

.propaganda1 p, .propaganda2 p, .propaganda3 p {
	font-family: 'Open Sans', Helvetica, Arial,
		sans-serif;
	color: #003333;
	text-align: center;
	margin: 10px;
}

.facebook header h1, .delivery h1 {
	font-family: 'Open Sans', Helvetica, Arial,
		sans-serif;
	font-size: 28px;
	color: white;
	text-transform: uppercase;
	text-align: center;
}

.pedido a[href ^="tel:"] {
	cursor: text;
	color: white;
	text-decoration: none;
}

.delivery a[href ^="tel:"] {
	cursor: text;
	color: #003333;
	text-decoration: none;
}

.delivery p {
	font-family: 'Open Sans', Helvetica, Arial,
		sans-serif;
	font-size: 18px;
	color: #003333;
	padding: 0 20px 0 25px;
}

.real {
	font-size: 26px;
}

.preco {
	font-size: 46px;
}

.centavos {
	font-size: 14px;
	position: absolute;
	padding-top: 10px;
}

.apetite {
	font-family: 'Open Sans', Helvetica, Arial, sans-serif;
	font-size: 36px;
	font-weight: bold;
	text-align: center;
	color: #FFCC00;
	text-transform: uppercase;
	margin: 0;
	padding-top: 20px;
}

.promocao1 {
	font-family: 'Open Sans', Helvetica, Arial, sans-serif;
	font-size: 16px;
	color: white;
}

 .promocao2 {
 	font-family: 'Open Sans', Helvetica, Arial, sans-serif;
